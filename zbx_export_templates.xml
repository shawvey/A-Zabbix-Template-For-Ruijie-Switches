<?xml version="1.0" encoding="UTF-8"?>
<zabbix_export>
    <version>5.0</version>
    <date>2021-12-10T09:00:06Z</date>
    <groups>
        <group>
            <name>交换机</name>
        </group>
    </groups>
    <templates>
        <template>
            <template>Universal_Switch_Template</template>
            <name>交换机通用模板</name>
            <groups>
                <group>
                    <name>交换机</name>
                </group>
            </groups>
            <applications>
                <application>
                    <name>CPU使用率</name>
                </application>
                <application>
                    <name>interface_流量</name>
                </application>
                <application>
                    <name>interface_状态</name>
                </application>
                <application>
                    <name>上层协议需要发送给一个网络单播地址的报文数</name>
                </application>
                <application>
                    <name>上层协议需要发送给一个非单播地址的报文数</name>
                </application>
                <application>
                    <name>介质相关的ip地址以及对应物理地址</name>
                </application>
                <application>
                    <name>传递给上层网络协议的非单播报文数</name>
                </application>
                <application>
                    <name>内存使用率</name>
                </application>
                <application>
                    <name>指向每个特定映射</name>
                </application>
                <application>
                    <name>接口数据链路地址</name>
                </application>
                <application>
                    <name>接口最后更新成当前操作状态的时刻</name>
                </application>
                <application>
                    <name>接口类型</name>
                </application>
                <application>
                    <name>接口类型描述</name>
                </application>
                <application>
                    <name>接口速率</name>
                </application>
                <application>
                    <name>流入的错误报文数</name>
                </application>
                <application>
                    <name>由于未知或不支持的网络协议而丢弃的输入报文的数量</name>
                </application>
                <application>
                    <name>由于错误而不能发送的报文数量</name>
                </application>
                <application>
                    <name>被丢弃的输入报文数</name>
                </application>
                <application>
                    <name>通过上层协议传递到子网的单播报文数</name>
                </application>
            </applications>
            <discovery_rules>
                <discovery_rule>
                    <name>atNetAddress Discovery</name>
                    <type>SNMP_AGENT</type>
                    <snmp_oid>discovery[{#SNMPVALUE},SNMPv2-SMI::mib-2.3.1.1.3]</snmp_oid>
                    <key>atNetAddress</key>
                    <item_prototypes>
                        <item_prototype>
                            <name>Special mapping $1</name>
                            <type>SNMP_AGENT</type>
                            <snmp_oid>SNMPv2-SMI::mib-2.3.1.1.1.{#SNMPINDEX}</snmp_oid>
                            <key>atIfIndex[{#SNMPVALUE}]</key>
                            <description>指向每个特定映射</description>
                            <applications>
                                <application>
                                    <name>指向每个特定映射</name>
                                </application>
                            </applications>
                        </item_prototype>
                        <item_prototype>
                            <name>At net ip address and physical address $1</name>
                            <type>SNMP_AGENT</type>
                            <snmp_oid>SNMPv2-SMI::mib-2.3.1.1.2.{#SNMPINDEX}</snmp_oid>
                            <key>atNetAddress[{#SNMPVALUE}]</key>
                            <trends>0</trends>
                            <value_type>CHAR</value_type>
                            <description>介质相关物理地址和所关联的IP地址</description>
                            <applications>
                                <application>
                                    <name>介质相关的ip地址以及对应物理地址</name>
                                </application>
                            </applications>
                        </item_prototype>
                    </item_prototypes>
                </discovery_rule>
                <discovery_rule>
                    <name>Network interfaces Discovery</name>
                    <type>SNMP_AGENT</type>
                    <snmp_oid>discovery[{#SNMPVALUE},IF-MIB::ifDescr]</snmp_oid>
                    <key>ifDescr</key>
                    <delay>30s</delay>
                    <item_prototypes>
                        <item_prototype>
                            <name>Administration status on interface $1</name>
                            <type>SNMP_AGENT</type>
                            <snmp_oid>IF-MIB::ifAdminStatus.{#SNMPINDEX}</snmp_oid>
                            <key>ifAdminStatus[{#SNMPVALUE}]</key>
                            <delay>30s</delay>
                            <trends>0</trends>
                            <value_type>CHAR</value_type>
                            <description>对应接口的管理状态</description>
                            <applications>
                                <application>
                                    <name>interface_状态</name>
                                </application>
                            </applications>
                        </item_prototype>
                        <item_prototype>
                            <name>Discard input packets on interface $1</name>
                            <type>SNMP_AGENT</type>
                            <snmp_oid>IF-MIB::ifInDiscards.{#SNMPINDEX}</snmp_oid>
                            <key>ifInDiscards[{#SNMPVALUE}]</key>
                            <delay>30s</delay>
                            <units>pkts/s</units>
                            <description>被丢弃（尽管没有错误）的输入报文数，并且这些报文不会被传递给上层网络协议。</description>
                            <applications>
                                <application>
                                    <name>被丢弃的输入报文数</name>
                                </application>
                            </applications>
                        </item_prototype>
                        <item_prototype>
                            <name>Input error packets on interface $1</name>
                            <type>SNMP_AGENT</type>
                            <snmp_oid>IF-MIB::ifInErrors.{#SNMPINDEX}</snmp_oid>
                            <key>ifInErrors[{#SNMPVALUE}]</key>
                            <delay>30s</delay>
                            <units>pkts/s</units>
                            <description>对应接口流入的错误报文数，由于错误使得这些报文不会被传递给上层网络协议。</description>
                            <applications>
                                <application>
                                    <name>流入的错误报文数</name>
                                </application>
                            </applications>
                        </item_prototype>
                        <item_prototype>
                            <name>In non-unicast packets on interface $1</name>
                            <type>SNMP_AGENT</type>
                            <snmp_oid>IF-MIB::ifInNUcastPkts.{#SNMPINDEX}</snmp_oid>
                            <key>ifInNUcastPkts[{#SNMPVALUE}]</key>
                            <delay>30s</delay>
                            <units>pkts/s</units>
                            <description>对应接口传递给上层网络协议的非单播报文数</description>
                            <applications>
                                <application>
                                    <name>传递给上层网络协议的非单播报文数</name>
                                </application>
                            </applications>
                        </item_prototype>
                        <item_prototype>
                            <name>Inconfig traffic on interface $1</name>
                            <type>SNMP_AGENT</type>
                            <snmp_oid>IF-MIB::ifInOctets.{#SNMPINDEX}</snmp_oid>
                            <key>ifInOctets[{#SNMPVALUE}]</key>
                            <delay>30s</delay>
                            <units>bps</units>
                            <description>对应接口输入的字节数</description>
                            <applications>
                                <application>
                                    <name>interface_流量</name>
                                </application>
                            </applications>
                            <preprocessing>
                                <step>
                                    <type>MULTIPLIER</type>
                                    <params>8</params>
                                    <error_handler>DISCARD_VALUE</error_handler>
                                </step>
                                <step>
                                    <type>CHANGE_PER_SECOND</type>
                                    <params/>
                                    <error_handler>DISCARD_VALUE</error_handler>
                                </step>
                            </preprocessing>
                        </item_prototype>
                        <item_prototype>
                            <name>In unicast packets on interface $1</name>
                            <type>SNMP_AGENT</type>
                            <snmp_oid>IF-MIB::ifInUcastPkts.{#SNMPINDEX}</snmp_oid>
                            <key>ifInUcastPkts[{#SNMPVALUE}]</key>
                            <delay>30s</delay>
                            <units>pkts/s</units>
                            <description>对应接口通过上层协议传递到子网的单播报文数</description>
                            <applications>
                                <application>
                                    <name>通过上层协议传递到子网的单播报文数</name>
                                </application>
                            </applications>
                        </item_prototype>
                        <item_prototype>
                            <name>Discard input packets due to unknownprotos on interface $1</name>
                            <type>SNMP_AGENT</type>
                            <snmp_oid>IF-MIB::ifInUnknownProtos.{#SNMPINDEX}</snmp_oid>
                            <key>ifInUnknownProtos[{#SNMPVALUE}]</key>
                            <delay>30s</delay>
                            <units>pkts/s</units>
                            <description>由于未知或不支持的网络协议而丢弃的输入报文的数量</description>
                            <applications>
                                <application>
                                    <name>由于未知或不支持的网络协议而丢弃的输入报文的数量</name>
                                </application>
                            </applications>
                        </item_prototype>
                        <item_prototype>
                            <name>Time of last change on interface $1</name>
                            <type>SNMP_AGENT</type>
                            <snmp_oid>IF-MIB::ifLastChange.{#SNMPINDEX}</snmp_oid>
                            <key>ifLastChange[{#SNMPVALUE}]</key>
                            <delay>30s</delay>
                            <units>uptime</units>
                            <description>对应接口最后更新成当前操作状态的时刻</description>
                            <applications>
                                <application>
                                    <name>接口最后更新成当前操作状态的时刻</name>
                                </application>
                            </applications>
                            <preprocessing>
                                <step>
                                    <type>MULTIPLIER</type>
                                    <params>0.01</params>
                                    <error_handler>DISCARD_VALUE</error_handler>
                                </step>
                            </preprocessing>
                        </item_prototype>
                        <item_prototype>
                            <name>Operation status on interface $1</name>
                            <type>SNMP_AGENT</type>
                            <snmp_oid>IF-MIB::ifOperStatus.{#SNMPINDEX}</snmp_oid>
                            <key>ifOperStatus[{#SNMPVALUE}]</key>
                            <delay>30s</delay>
                            <description>对应接口的操作状态</description>
                            <applications>
                                <application>
                                    <name>interface_状态</name>
                                </application>
                            </applications>
                        </item_prototype>
                        <item_prototype>
                            <name>Packets cannot be sent due to errors on interface $1</name>
                            <type>SNMP_AGENT</type>
                            <snmp_oid>IF-MIB::ifOutErrors.{#SNMPINDEX}</snmp_oid>
                            <key>ifOutErrors[{#SNMPVALUE}]</key>
                            <delay>30s</delay>
                            <units>pkts/s</units>
                            <description>对应接口由于错误而不能发送的报文数量</description>
                            <applications>
                                <application>
                                    <name>由于错误而不能发送的报文数量</name>
                                </application>
                            </applications>
                        </item_prototype>
                        <item_prototype>
                            <name>Out non-unicast packets on interface $1</name>
                            <type>SNMP_AGENT</type>
                            <snmp_oid>IF-MIB::ifOutNUcastPkts.{#SNMPINDEX}</snmp_oid>
                            <key>ifOutNUcastPkts[{#SNMPVALUE}]</key>
                            <delay>30s</delay>
                            <units>pkts/s</units>
                            <description>上层协议（如IP）需要发送给一个非单播地址的报文数，该数量包括丢弃的或因为某种原因未发送的报文数</description>
                            <applications>
                                <application>
                                    <name>上层协议需要发送给一个非单播地址的报文数</name>
                                </application>
                            </applications>
                        </item_prototype>
                        <item_prototype>
                            <name>Outconfig traffic on interface $1</name>
                            <type>SNMP_AGENT</type>
                            <snmp_oid>IF-MIB::ifOutOctets.{#SNMPINDEX}</snmp_oid>
                            <key>ifOutOctets[{#SNMPVALUE}]</key>
                            <delay>30s</delay>
                            <units>bps</units>
                            <description>对应接口输出的字节数</description>
                            <applications>
                                <application>
                                    <name>interface_流量</name>
                                </application>
                            </applications>
                            <preprocessing>
                                <step>
                                    <type>MULTIPLIER</type>
                                    <params>8</params>
                                    <error_handler>DISCARD_VALUE</error_handler>
                                </step>
                                <step>
                                    <type>CHANGE_PER_SECOND</type>
                                    <params/>
                                    <error_handler>DISCARD_VALUE</error_handler>
                                </step>
                            </preprocessing>
                        </item_prototype>
                        <item_prototype>
                            <name>Out unicast packets on interface $1</name>
                            <type>SNMP_AGENT</type>
                            <snmp_oid>IF-MIB::ifOutUcastPkts.{#SNMPINDEX}</snmp_oid>
                            <key>ifOutUcastPkts[{#SNMPVALUE}]</key>
                            <delay>30s</delay>
                            <units>pkts/s</units>
                            <description>上层协议（如IP）需要发送给一个网络单播地址的报文数，该数量包括丢弃的或未发送的报文数</description>
                            <applications>
                                <application>
                                    <name>上层协议需要发送给一个网络单播地址的报文数</name>
                                </application>
                            </applications>
                        </item_prototype>
                        <item_prototype>
                            <name>Physcial address of interface $1</name>
                            <type>SNMP_AGENT</type>
                            <snmp_oid>IF-MIB::ifPhysAddress.{#SNMPINDEX}</snmp_oid>
                            <key>ifPhysAddress[{#SNMPVALUE}]</key>
                            <delay>30s</delay>
                            <trends>0</trends>
                            <value_type>CHAR</value_type>
                            <description>对应接口的数据链路地址</description>
                            <applications>
                                <application>
                                    <name>接口数据链路地址</name>
                                </application>
                            </applications>
                        </item_prototype>
                        <item_prototype>
                            <name>Speed on interface $1</name>
                            <type>SNMP_AGENT</type>
                            <snmp_oid>IF-MIB::ifSpeed.{#SNMPINDEX}</snmp_oid>
                            <key>ifSpeed[{#SNMPVALUE}]</key>
                            <delay>30s</delay>
                            <units>bps/s</units>
                            <description>对应接口输出的字节数</description>
                            <applications>
                                <application>
                                    <name>接口速率</name>
                                </application>
                            </applications>
                            <preprocessing>
                                <step>
                                    <type>MULTIPLIER</type>
                                    <params>8</params>
                                    <error_handler>DISCARD_VALUE</error_handler>
                                </step>
                            </preprocessing>
                        </item_prototype>
                    </item_prototypes>
                    <graph_prototypes>
                        <graph_prototype>
                            <name>Traffic on interface {#SNMPVALUE}</name>
                            <graph_items>
                                <graph_item>
                                    <sortorder>1</sortorder>
                                    <color>1A7C11</color>
                                    <item>
                                        <host>Universal_Switch_Template</host>
                                        <key>ifInOctets[{#SNMPVALUE}]</key>
                                    </item>
                                </graph_item>
                                <graph_item>
                                    <sortorder>2</sortorder>
                                    <color>F63100</color>
                                    <item>
                                        <host>Universal_Switch_Template</host>
                                        <key>ifOutOctets[{#SNMPVALUE}]</key>
                                    </item>
                                </graph_item>
                            </graph_items>
                        </graph_prototype>
                    </graph_prototypes>
                </discovery_rule>
            </discovery_rules>
            <screens>
                <screen>
                    <name>锐捷交换机CPU与内存</name>
                    <hsize>1</hsize>
                    <vsize>2</vsize>
                </screen>
            </screens>
        </template>
    </templates>
</zabbix_export>
